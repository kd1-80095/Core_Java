package com.assignment9_3;

public class Student {

	private int roll;
	private String name;
	private String city;
	private double marks;
	
	public Student() {
		this.roll = 0;
		this.name = "";
		this.city = "";
		this.marks = 0;
	}
	
	public Student(int roll, String name, String city, double marks) {
		this.roll = roll;
		this.name = name;
		this.city = city;
		this.marks = marks;
	}
	
	
	
}
